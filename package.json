    "command-exists": "^1.2.8",
    "got": "^11.0.0",
    "guid-typescript": "^1.0.9",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "opener": "^1.5.1",
    "portfinder": "^1.0.28",
    "telaug": "0.0.13",
    "tmp": "^0.2.1",
    "username": "^5.1.0",
    "vscode-kubernetes-tools-api": "^1.3.0",
